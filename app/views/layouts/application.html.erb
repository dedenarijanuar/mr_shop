    <script src="/assets/idhub/js/app.js" type="text/javascript"></script>

    <!--end::Page Snippets -->

    <!--begin::Page Resources -->
    <script src="/assets/demo/default/custom/components/base/sweetalert2.js" type="text/javascript"></script>
<script>
jQuery(document).ready(function() {
  del = $('.la-trash-o').parent();
  link = del.attr('href')
  $('.la-trash-o').parent().attr('href', '#')
  $('.la-trash-o').parent().click(function(){
    result = confirm("Apa Kamu Yakin?");
    if (result) {
      location.href = link;
    }
  })

$('.search_name').keyup(function(){
  f = $('.search_name').val()
  v = location.origin + location.pathname
  setTimeout(function() {
    location.href = v+'?find='+f
  }, 2000);

})
   <% if flash[:notice] || flash[:error] %>
     swal(
      '<%= flash[:notice] || flash[:error] %>',
      '',
      'info'
    )
   <% end %>
});
</script>
    <!--end::Page Resources -->

  </body>

  <!-- end::Body -->
</html>